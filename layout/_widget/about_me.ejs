<% if (theme.about_me){ %>
<div class="widget-wrap">
  <h3 class="widget-title"><%= theme.about_me.title || __('about_me') %></h3>
  <ul class="widget about-me">
    <% theme.about_me.ads.forEach(function(item){ %>
    <% banner = item.split(",")[0] %>
    <% link = item.split(",")[1] %>
    <li><a href="<%= link %>" target="_blank"><img class="ad"  src="<%= banner %>" /></a></li>
    <% }); %>
    <% theme.about_me.texts.forEach(function(item){ %>
    <li align=left><%- item %></li>
    <% }); %>
  </ul>
</div>
<% } %>
